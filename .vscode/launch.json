{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/src",
      "runtimeExecutable": "denon",
      "runtimeArgs": [
        "run",
        "--inspect-brk",
        "-A",
        "--config=${workspaceFolder}/tsconfig.json",
        "--unstable",
        "--importmap=import-map.json",
        "app.ts"
      ],
      "port": 9229
    }
  ]
}
